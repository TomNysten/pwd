{% extends './layout.html.twig' %}

{% block title %}Sets{% endblock %}

{% block content %}
    <h1>Sets</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ set.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ set.name }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ set.year }}</td>
            </tr>
            <tr>
                <th>Icon</th>
                <td>{{ set.icon }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sets_index') }}">back to list</a>

    <a href="{{ path('sets_edit', {'id': set.id}) }}">edit</a>

    {{ include('admin/sets/_delete_form.html.twig') }}
{% endblock %}
